{
    "name": "dpredusel/php-json-rpc",
    "description": "A simple PHP implementation of JSON-RPC protocol",
    "author": {
        "name": "David Predusel",
        "email": "preduseldavid@gmail.com"
    },
    "require": {
        "php": ">=7.0.0",
        "vlucas/phpdotenv": "^5.6",
        "ext-mysqli": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "JsonRpc\\App\\": "app",
            "JsonRpc\\Driver\\": "src\\Driver",
            "JsonRpc\\Http\\": "src\\Http",
            "JsonRpc\\JsonRpc\\": "src\\JsonRpc"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonRpc\\JsonRpc\\Tests\\": "tests"
        }
    }
}
